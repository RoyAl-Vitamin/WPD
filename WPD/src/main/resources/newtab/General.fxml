<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="30.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mmsp.logic.newtab.FXMLCtrlGeneral">
   <children>
      <VBox fx:id="vbStudyLoad" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" minHeight="25.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" minHeight="25.0" text="Учебная нагрузка" />
                  <Button fx:id="bAddRowStudyLoad" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#clickBAddRowStudyLoad" text="+" />
                  <Button fx:id="bDeleteRowStudyLoad" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#clickBDeleteRowStudyLoad" text="-" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox fx:id="vbControlMesaures" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" minHeight="25.0" spacing="10.0">
               <children>
                  <Label text="Контрольные мероприятия" />
                  <Button fx:id="bAddRowControlMesaures" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#clickBAddRowControlMesaures" text="+" />
                  <Button fx:id="bDeleteRowControlMesaures" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#clickBDeleteRowControlMesaures" text="-" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
